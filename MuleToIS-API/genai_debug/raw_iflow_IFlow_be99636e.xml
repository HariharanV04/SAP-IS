            <bpmn2:extensionElements>
                <ifl:property>
                    <key>bodyType</key>
                    <value>expression</value>
                </ifl:property>
                <ifl:property>
                    <key>propertyTable</key>
                    <value></value>
                </ifl:property>
                <ifl:property>
                    <key>headerTable</key>
                    <value></value>
                </ifl:property>
                <ifl:property>
                    <key>wrapContent</key>
