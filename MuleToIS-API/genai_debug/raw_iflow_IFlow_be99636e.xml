                <ifl:property>
                    <key>componentVersion</key>
                    <value>1.4</value>
                </ifl:property>
                <ifl:property>
                    <key>activityType</key>
                    <value>Enricher</value>
                </ifl:property>
                <ifl:property>
                    <key>cmdVariantUri</key>
                    <value>ctype::FlowstepVariant/cname::Enricher/version::1.4.2</value>
                </ifl:property>
                <ifl:property>
                    <key>bodyContent</key>
                    <value>def productIdentifiers = property.get('odata.productIdentifiers').split(',');
def requestedProductId = message.getHeaders().get('productIdentifier');
def isValid = productIdentifiers.contains(requestedProductId);
message.setProperty('isExistProduct', isValid);
return message;</value>
                </ifl:property>
            </bpmn2:extensionElements>
